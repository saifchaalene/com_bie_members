    <field name="start_date" type="mycalendar" class="inputbox" required="true" readonly="false" label="COM_BIE_DELEGATE_LBL_DELEGATE_START_DATE" description="COM_BIE_DELEGATE_LBL_DELEGATE_START_DATE" hint="COM_BIE_DELEGATE_LBL_DELEGATE_START_DATE" format="%d/%m/%Y"/>
    <field name="ordering"/>
