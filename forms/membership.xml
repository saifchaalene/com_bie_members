    <field name="modified_by" type="modifiedby" default="0" label="COM_CRM_CONTACTS_FORM_LBL_INDIVIDUAL_MODIFIED_BY" description="COM_CRM_CONTACTS_FORM_DESC_INDIVIDUAL_MODIFIED_BY" hidden="true" hint="COM_CRM_CONTACTS_FORM_LBL_INDIVIDUAL_MODIFIED_BY"/>
    <field name="version_note" type="text" label="JGLOBAL_FIELD_VERSION_NOTE_LABEL" description="JGLOBAL_FIELD_VERSION_NOTE_DESC" class="inputbox" size="45" labelclass="control-label"/>
  </fieldset>
</form>
